{
    "name": "vite-plugin-book",
    "version": "0.0.2",
    "description": "A vite plugin to help you write a book.",
    "module": "true",
    "main": "./dist/render/index.js",
    "types": "./dist/render/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/render/index.js",
            "types": "./dist/render/index.d.ts"
        },
        "./*": "./dist/render/*",
        "./vite": {
            "require": "./dist/plugin/index.js",
            "types": "./dist/plugin/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "vite": [
                "dist/plugin/index.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "commit": "git-cz",
        "dev": "concurrently -n plugin,ui,render \"pnpm run dev:plugin\" \"pnpm run dev:ui\" \"pnpm run dev:render\"",
        "dev:ui": "vite build --config vite.config.ui.ts --watch",
        "dev:plugin": "rollup -c --watch",
        "dev:render": "vite build --config vite.config.render.ts --watch",
        "build": "concurrently -n plugin,ui,render \"pnpm run build:plugin\" \"pnpm run build:ui\" \"pnpm run build:render\"",
        "build:ui": "vite build --config vite.config.ui.ts",
        "build:plugin": "rollup -c",
        "build:render": "vite build --config vite.config.render.ts"
    },
    "keywords": [
        "vite-plugin"
    ],
    "author": "Saul-Mirone",
    "license": "MIT",
    "devDependencies": {
        "@milkdown/core": "^5.4.1",
        "@milkdown/plugin-history": "^5.4.1",
        "@milkdown/plugin-listener": "^5.4.1",
        "@milkdown/plugin-prism": "^5.4.1",
        "@milkdown/plugin-slash": "^5.4.1",
        "@milkdown/plugin-tooltip": "^5.4.1",
        "@milkdown/preset-commonmark": "^5.4.1",
        "@milkdown/preset-gfm": "^5.4.1",
        "@milkdown/prose": "^5.4.1",
        "@milkdown/theme-nord": "^5.4.1",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^17.0.8",
        "@types/react": "17.0.38",
        "@types/react-dom": "^17.0.9",
        "@types/sortablejs": "^1.10.7",
        "@types/ws": "^8.2.2",
        "@vitejs/plugin-react": "^1.1.4",
        "immer": "^9.0.12",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "react-split-pane": "^0.1.92",
        "sortablejs": "^1.14.0",
        "vite-plugin-windicss": "^1.6.3",
        "windicss": "^3.4.3"
    },
    "dependencies": {
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/pluginutils": "^4.1.2",
        "birpc": "^0.1.0",
        "flatted": "^3.2.4",
        "fs-extra": "^10.0.0",
        "magic-string": "^0.25.7",
        "pathe": "^0.2.0",
        "react-sortablejs": "^6.0.3",
        "sirv": "^2.0.0",
        "ws": "^8.4.2"
    }
}
