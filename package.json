{
    "name": "vite-plugin-book",
    "version": "0.0.2",
    "description": "A vite plugin to help you write a book.",
    "module": "true",
    "main": "./dist/render/index.js",
    "types": "./dist/render/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/render/index.js",
            "types": "./dist/render/index.d.ts"
        },
        "./*": "./dist/render/*",
        "./vite": {
            "require": "./dist/plugin/index.js",
            "types": "./dist/plugin/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "vite": [
                "dist/plugin/index.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "commit": "git-cz",
        "start": "vite",
        "example:build": "vite build",
        "example:preview": "vite preview --port 7001",
        "build": "concurrently -n plugin,ui,render \"pnpm run build:plugin\" \"pnpm run build:ui\" \"pnpm run build:render\"",
        "build:ui": "vite build --config vite.config.ui.ts",
        "build:plugin": "rollup -c",
        "build:render": "vite build --config vite.config.render.ts"
    },
    "keywords": [
        "vite-plugin"
    ],
    "author": "Saul-Mirone",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^16.0.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@milkdown/core": "^5.3.2",
        "@milkdown/plugin-listener": "^5.3.2",
        "@milkdown/plugin-slash": "^5.3.2",
        "@milkdown/plugin-tooltip": "^5.3.2",
        "@milkdown/preset-gfm": "^5.3.2",
        "@milkdown/prose": "^5.3.2",
        "@milkdown/theme-nord": "^5.3.2",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^17.0.8",
        "@types/react": "17.0.38",
        "@types/react-dom": "^17.0.9",
        "@types/ws": "^8.2.2",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "@unocss/reset": "^0.22.4",
        "@vitejs/plugin-react": "^1.1.4",
        "concurrently": "^7.0.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "git-cz": "^4.7.6",
        "husky": "^7.0.2",
        "lint-staged": "^12.0.0",
        "prettier": "^2.3.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "react-split-pane": "^0.1.92",
        "rollup": "^2.64.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-dts": "^4.1.0",
        "rollup-plugin-esbuild": "^4.8.2",
        "typescript": "^4.5.4",
        "unocss": "^0.22.2",
        "vite": "^2.5.3"
    },
    "dependencies": {
        "@rollup/pluginutils": "^4.1.2",
        "birpc": "^0.1.0",
        "flatted": "^3.2.4",
        "fs-extra": "^10.0.0",
        "magic-string": "^0.25.7",
        "pathe": "^0.2.0",
        "sirv": "^2.0.0",
        "ws": "^8.4.2"
    }
}
